.addButton {
  position: fixed;
  opacity: 0.7;
  z-index: 2;
  right: 10rpx;
  bottom: 12%;
}

/* 模态框 */
.modal{
  position: absolute;
  left: 0rpx;
  top: 100rpx;
  z-index: 200;
  width:100%;
}

.modal-content{
  width: 90%;
  height: 900rpx;
  margin: 0 auto;
  background: #fff;
  font-size: 30rpx;
  font-family: "微软雅黑";
  padding: 20rpx;
  border-radius: 5px;
}

.modal-line-box{
  margin: 30rpx 0rpx;
  align-content: center;
}
.modal-line-box.amount-title{
  text-align: center;
  font-size: 40rpx;
}
.modal-line-box.side-bord{
 margin-bottom: 50rpx;  
}
.modal-line-box.side-bord picker,.has-bord{
  border:1px solid #eee;
  height: 70rpx;
  width: 65%;
  padding-left:20rpx; 
}

.modal-line-box.side-bord .has-bord-danger{
  border:1px solid #FF617C;
  height: 70rpx;
  width: 65%;
  padding-left:20rpx; 
}
.modal-line-box.amount-info{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.modal-line-box.amount-checkbox{
  display: flex;
  flex-direction: column;
  /* justify-content: space-between; */
}
.modal-line-box.amount-info2{
  text-align: center;
  background: #eee;
  font-size: 23rpx;
  padding: 20rpx;
  border-radius: 5px;
  margin-top: 50rpx;
}
.modal-line-box .amount-style{
  color: #FF617C;
}

button {
  border: none;
  color: white;
  font-family:Arial;
  text-align: center;
  text-decoration: none;
  width: 40%;
  font-size: 14px;
  
}

button.bg-success{
  background-color: #00a2e9 !important;
}
button.bg-cancle{
  background-color: #cdcdcd !important;
}

.mask{
  position: fixed;
  top: 0px;
  left: 0px;
  width: 100%;
  height:100%;
  z-index: 100;
  background:rgba(0,0,0,0.5);
}
.showMask,.showModal{
  display: block;
}
.hideMask,.hideModal{
  display: none;
}
.picker-detail{
  line-height: 70rpx;
  width: 300rpx;
}
